{
  "compilerOptions": {
    "moduleResolution": "node",
    // `import React` instead of `import * as React`
     "allowSyntheticDefaultImports": true,
     // <div /> => React.createElement("div")
    "incremental": true,
    "outDir":"./.ts-cashe",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "noImplicitAny": false
  },
}
